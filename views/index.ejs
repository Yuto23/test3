<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>SAMPLE</title>
  </head>
  <body>
    <main>
        <label>Name:</label>
        <input type="text" id="name-input">
        <button>Add Name</button>
    </main>
    <script src="https://unpkg.com/canvas-datagrid"></script>
    <div id="test" />
    <script>
        // create a new grid
        var grid = canvasDatagrid({
        parentNode: document.getElementById('test')
        });
    grid.data = [
    <% dt.forEach((item) => { %>
         {id:<%= item.id %>, name:'<%= item.name %>'},
        <% }) %>
        ];
    </script>
  </body>
</html>
